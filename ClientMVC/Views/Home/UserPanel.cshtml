@{
    ViewData["Title"] = "UserPanel";
}

<h1>UserPanel</h1>

<table class="table table-bordered">

    <tr>
        <th>Type</th>
        <th>Value</th>
    </tr>
    <tbody>
        @foreach(var item in User.Claims)
        {
            <tr>
                <td>@item.Type</td>
                <td>@item.Value</td>
            </tr>
        }
    </tbody>

</table>
